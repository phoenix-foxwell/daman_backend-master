  <script>
    $(document).on("click", ".card-body", function (e) {
      document.querySelector('.card .select').classList.remove('select')
      e.currentTarget.classList.add("select");
    })

    $.ajax({
      type: "POST",
      url: "/membership/get_membership_types",
      success(result) {
        if (!result.error) {
          let list_elemet = document.getElementById("membership_list");
          let html = ``;
          for (let i = 0; i < result.data.length; i++) {
            const element = result.data[i];
            if (i == 0) {
              html += `<div class="col-12 col-sm-6 col-lg-4 card" style="cursor: pointer;">
              <div class="card-body select" id="`+ element.id + `">
                <div class="yes"><i class="fa-solid fa-check"></i></div>
                <h3 class="card-title">`+ element.membership_type + `</h3>
                <p>`+ element.description + `</p>
              </div>
            </div>`;
              window.localStorage.membership_id = element.id
            } else {
              html += `<div class="col-12 col-sm-6 col-lg-4 card" style="cursor: pointer;">
              <div class="card-body" id="`+ element.id + `">
                <div class="yes"><i class="fa-solid fa-check"></i></div>
                <h3 class="card-title">`+ element.membership_type + `</h3>
                <p>`+ element.description + `</p>
              </div>
            </div>`;
            }
          }
          list_elemet.innerHTML = html;
        }
      },
      error(err) {

        myFunction(err.statusText);
      }
    });
    $(document).on("click", "#apply", function (e) {
      let selected = document.querySelector(".select");
      window.localStorage.membership_id = selected.id;
      window.location.href = "/pages/register"
    })
  </script>
